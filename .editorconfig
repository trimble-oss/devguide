# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
insert_final_newline = true

[*.min.js]
insert_final_newline = false

[cspell.json]
indent_size = 4
insert_final_newline = false

[*.svg]
insert_final_newline = false

[CNAME]
insert_final_newline = false
