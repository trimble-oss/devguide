{
  "root": true,
  "env": {
    "browser": true,
    "jquery": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": [
    "google",
    "prettier"
  ],
  "rules": {
    "curly": 0,
    "no-invalid-this": 0,
    "no-var": 0,
    "one-var": 0
  }
}
